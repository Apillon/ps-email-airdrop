  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.10",
    "@types/supertest": "^2.0.16",
    "ethers": "^6.9.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-mysql-migrate": "^1.0.2",
    "typescript": "^5.3.2"
